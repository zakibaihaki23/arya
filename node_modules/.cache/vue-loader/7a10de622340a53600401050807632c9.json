icon class=\"p-icon-nav\">\n                        <v-icon></v-icon>\n                        </v-list-item-icon>\n                        <v-list-item-title class=\"fs13\">Create Packing</v-list-item-title>\n                    </template>\n\n            <v-col\n              cols=\"12\"\n              sm=\"12\"\n            >\n              <v-text-field\n                color=\"green\"\n                v-model=\"form.PackingCode\"\n                label=\"PACKING CODE\"\n              ></v-text-field>\n              <v-text-field\n                color=\"green\"\n                v-model=\"form.DeliveryDate\"\n                label=\"DELIVERY DATE\"\n              ></v-text-field>\n              <v-text-field\n                color=\"green\"\n                v-model=\"form.Status\"\n                label=\"STATUS\"\n              ></v-text-field>\n            </v-col>\n            \n            <v-btn\n              style=\"color:white\"\n              color=\"green\"\n              @click=\"submitForm()\"\n            >\n              TAMBAH\n            </v-btn>\n        </v-list-group>\n        <br><br>\n      </v-col>\n\n      </v-row>\n      \n        <!-- ISI TABEL -->\n\n         <!-- NAMPILIN SEMUA DATA\n        <pre>{{tableData}}</pre> -->\n\n          <v-data-table \n            :headers=\"table\"\n            :items=\"tableData\"\n            :items-per-page=\"10\"\n          >\n            <template v-slot:tableData=\"props\">\n              <tr>\n                <td>{{ props.tableData.document_code }}</td>\n                <td>{{ props.tableData.delivery_date }}</td>\n                <td>{{ props.tableData.Status }}</td>\n                <td>{{ props.tableData.Action }}</td>\n              </tr>\n            </template>\n          </v-data-table>\n      </v-col>\n\n    </v-row>\n  </div>  \n</template> \n\n<script>\n  import axios from 'axios'\n  import FilterArea from '../../components/Filter/FilterArea.vue'\n  import FilterWarehouse from '../../components/Filter/FilterWarehouse.vue'\n  import FilterDeliveryDate from '../../components/Filter/FilterDeliveryDate.vue'\n  import FilterSearch from '../../components/Filter/FilterSearch.vue'\n  import Navbar from '../../components/Navbar.vue'\n  \n  export default {\n  components: { FilterArea, FilterWarehouse, FilterDeliveryDate, FilterSearch, Navbar,  },\n    name: 'ListPacking',\n    data() {\n      return {\n        title:'',\n        author:'',\n        form:{},\n        table: [\n          {\n            text: 'PACKING CODE',\n            value: 'document_code',\n          },\n          {\n            text: 'DELIVERY DATE',\n            value: 'delivery_date',\n          },\n          {\n            text: 'STATUS',\n            value: 'Status',\n          },\n          {\n            text: 'ACTION',\n            value: 'Action',\n          },\n        ],\n        tableData: [],\n      }\n    },\n    methods: {\n      renderData() {\n        let that = this\n        axios\n\n          //UNTUK MENGAMBIL & MENAMPILKAN DATA\n          .get('http://10.10.0.38:8083/v1/packing')\n          \n          .then((response) => {\n            console.log(response.data)\n            that.tableData = response.data\n          })\n          \n          .catch((error) => {\n            console.log(error)\n          })\n          },\n\n          // UNTUK MEMASUKAN DATA\n          submitForm(){\n            axios.post('https://603db17548171b0017b2d7f0.mockapi.io/Helper', this.form)\n          .then(function (response) {\n            console.log(response);\n            })\n          .catch(function (error) {\n            console.log(error);\n              });\n          }\n    },\n    created() {\n      this.renderData()\n    },\n  }\n</script>\n\n<style scoped>\n  .isitbl {\n    height: 100%;\n    width: 100%;\n    position: relative;\n    top: 0;\n    right: 0;\n  }\n</style>"]}]}