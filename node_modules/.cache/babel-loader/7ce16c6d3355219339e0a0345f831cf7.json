},\n    name: 'apitest',\n    data() {\n      return {\n        title:'',\n        author:'',\n        form:{},\n        table: [\n          {\n            text: 'Id',\n            value: 'id',\n          },\n          {\n            text: 'NAMA HELPER',\n            value: 'Name',\n          },\n          {\n            text: 'NOMOR HP',\n            value: 'PhoneNumber',\n          },\n          {\n            text: 'ALAMAT',\n            value: 'Address',\n          },\n          {\n            text: 'TIPE',\n            value: 'Type',\n          },\n          {\n            text: 'WAREHOUSE',\n            value: 'Warehouse',\n          },\n          {\n            text: 'STATUS',\n            value: 'Status',\n          },\n        ],\n        tableData: [],\n      }\n    },\n    methods: {\n      renderData() {\n        let that = this\n        axios\n\n          //UNTUK MENGAMBIL & MENAMPILKAN DATA\n          .get('https://603db17548171b0017b2d7f0.mockapi.io/Helper')\n          .then((response) => {\n            console.log(response.data)\n            that.tableData = response.data\n          })\n          \n          .catch((error) => {\n            console.log(error)\n          })\n          },\n\n          // UNTUK MEMASUKAN DATA\n          submitForm(){\n            axios.post('https://603db17548171b0017b2d7f0.mockapi.io/Helper', this.form)\n          .then(function (response) {\n            console.log(response);\n            })\n          .catch(function (error) {\n            console.log(error);\n              });\n          }\n    },\n    created() {\n      this.renderData()\n    },\n  }\n</script>\n\n<style scoped>\n  .isitbl {\n    height: 100%;\n    width: 100%;\n    position: relative;\n    top: 0;\n    right: 0;\n  }\n</style>"],"sourceRoot":"src/views"}]}