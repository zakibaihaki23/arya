/v-icon>\n                        </v-list-item-icon>\n                        <v-list-item-title class=\"fs13\">Create Helper</v-list-item-title>\n                    </template>\n\n            <v-col\n              cols=\"12\"\n              sm=\"12\"\n            >\n              <v-text-field\n                color=\"green\"\n                v-model=\"form.Name\"\n                label=\"NAMA HELPER\"\n              ></v-text-field>\n              <v-text-field\n                color=\"green\"\n                v-model=\"form.PhoneNumber\"\n                label=\"NO HP\"\n              ></v-text-field>\n              <v-text-field\n                color=\"green\"\n                v-model=\"form.Type\"\n                label=\"ALAMAT\"\n              ></v-text-field>\n              <v-text-field\n                color=\"green\"\n                v-model=\"form.Warehouse\"\n                label=\"Werehouse\"\n              ></v-text-field>\n              <v-text-field\n                color=\"green\"\n                v-model=\"form.Status\"\n                label=\"Status\"\n              ></v-text-field>\n            </v-col>\n            \n            <v-btn\n              style=\"color:white\"\n              color=\"green\"\n              @click=\"submitForm()\"\n            >\n              TAMBAH\n            </v-btn>\n        </v-list-group>\n        <br><br>\n      </v-col>\n\n      </v-row>\n      \n        <!-- ISI TABEL -->\n\n         <!-- NAMPILIN SEMUA DATA\n        <pre>{{tableData}}</pre> -->\n\n          <v-data-table \n            :headers=\"table\"\n            :items=\"tableData\"\n            :items-per-page=\"10\"\n          >\n            <template v-slot:tableData=\"props\">\n              <tr>\n                <td>{{ props.tableData.Name }}</td>\n                <td>{{ props.tableData.PhoneNumber }}</td>\n                <td>{{ props.tableData.Address }}</td>\n                <td>{{ props.tableData.Type }}</td>\n                <td>{{ props.tableData.Warehouse }}</td>\n                <td>{{ props.tableData.Status }}</td>\n              </tr>\n            </template>\n          </v-data-table>\n      </v-col>\n\n    </v-row>\n  </div>  \n</template> \n\n<script>\n  import axios from 'axios'\n  import FilterArea from '../components/Filter/FilterArea.vue'\n  import FilterWarehouse from '../components/Filter/FilterWarehouse.vue'\n  import FilterSearch from '../components/Filter/FilterSearch.vue'\n  import Navbar from '../components/Navbar.vue'\n  // import Appbar from '../components/Appbar.vue'\n  \n  export default {\n  components: { Navbar, FilterArea, FilterWarehouse, FilterSearch, },\n    name: 'apitest',\n    data() {\n      return {\n        title:'',\n        author:'',\n        form:{},\n        table: [\n          {\n            text: 'Id',\n            value: 'id',\n          },\n          {\n            text: 'NAMA HELPER',\n            value: 'Name',\n          },\n          {\n            text: 'NOMOR HP',\n            value: 'PhoneNumber',\n          },\n          {\n            text: 'ALAMAT',\n            value: 'Address',\n          },\n          {\n            text: 'TIPE',\n            value: 'Type',\n          },\n          {\n            text: 'WAREHOUSE',\n            value: 'Warehouse',\n          },\n          {\n            text: 'STATUS',\n            value: 'Status',\n          },\n        ],\n        tableData: [],\n      }\n    },\n    methods: {\n      renderData() {\n        let that = this\n        axios\n\n          //UNTUK MENGAMBIL & MENAMPILKAN DATA\n          .get('https://603db17548171b0017b2d7f0.mockapi.io/Helper')\n          .then((response) => {\n            console.log(response.data)\n            that.tableData = response.data\n          })\n          \n          .catch((error) => {\n            console.log(error)\n          })\n          },\n\n          // UNTUK MEMASUKAN DATA\n          submitForm(){\n            axios.post('https://603db17548171b0017b2d7f0.mockapi.io/Helper', this.form)\n          .then(function (response) {\n            console.log(response);\n            })\n          .catch(function (error) {\n            console.log(error);\n              });\n          }\n    },\n    created() {\n      this.renderData()\n    },\n  }\n</script>\n\n<style scoped>\n  .isitbl {\n    height: 100%;\n    width: 100%;\n    position: relative;\n    top: 0;\n    right: 0;\n  }\n</style>"]}]}